		"express-session": "^1.17.2",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"morgan": "^1.10.0",
		"pg": "^8.6.0",
		"pg-format": "^1.0.4",
		"react-bootstrap": "^2.0.0-beta.4",
		"react-chartjs-2": "^3.0.4",
		"react-countup": "^4.4.0",
		"react-redux": "^7.2.4",
		"reactstrap": "^8.9.0",
		"redux": "^4.1.1",
