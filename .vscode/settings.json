{
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"files.eol": "\n",
	"cSpell.ignoreWords": [
		"min",
		"s",
		"slacktastic",
		"t",
		"v",
		"vnmvdqn",
		"xo",
		"xoxb",
		"yr"
	],
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	]
}
